{
  "name": "jasonbronson/linode-api",
  "description": "Linode API for updating home dns entry",
  "type": "library",
  "keywords": [
    "api",
    "linode",
    "home",
    "dns"
  ],
  "homepage": "https://github.com/jasonbronson/linode-api",
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "ext-curl": "*",
    "ext-json": "*",
    "vlucas/phpdotenv": "^3.1",
    "guzzlehttp/guzzle": "^6.3",
    "monolog/monolog": "^1.25",
    "webinarium/linode-api": "^1.0"
  },
  "require-dev": {},
  "autoload": {
    "psr-4": {
      "Linode\\": "src/"
    },
    "classmap": [
      "."
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Linode\\": "tests/"
    }
  },
  "config": {
    "bin-dir": "bin"
  }
}
